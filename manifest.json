{
   "action": {
      "default_icon": "icon-48.png",
      "default_popup": "popup.html",
      "default_title": "DivMagic"
   },
   "author": "DivMagic",
   "background": {
      "service_worker": "background.bundle.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+X",
            "mac": "Command+Shift+X"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "wordpress.bundle.js" ],
      "matches": [ "https://*.wordpress.com/wp-admin/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "devtools_page": "devtools.html",
   "homepage_url": "https://DivMagic.com",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_extName__",
   "optional_host_permissions": [ "https://*/*", "http://*/*" ],
   "options_page": "options.html",
   "permissions": [ "scripting", "activeTab", "storage", "contextMenus" ],
   "short_name": "DivMagic",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.20",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "blockRegistration.bundle.js" ]
   } ]
}
